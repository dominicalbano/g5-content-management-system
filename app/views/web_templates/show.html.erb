<% content_for(:title) { web_template.title } %>

<% content_for(:head) { stylesheet_link_tag(*web_template.stylesheet_link_paths) } %>

<% content_for(:head) { raw web_template.canonical_link_element } %>

<% content_for(:head_widgets) { raw head_widgets(web_template) } %>

<% content_for(:body_classes) { "page-#{web_template.slug}" } %>

<%= raw preview(web_template.website_layout, web_template) %>

<% content_for(:bottom_of_body) { javascript_include_tag(*web_template.javascripts) } %>
