<li><%= column_widget.name %>
  <ul>
    <% tags = %w(row_count) %>
    <table width="75%" border="1">
      <% tags.each do |tag| %>
          <tr>
            <th><%= tag %></th>
            <td><%= column_widget.get_setting_value(tag) %></td>
          </tr>
      <% end %>
    </table>
    <% children = Array.new %>
    <% (1..column_widget.max_widgets).each do |i| %>
      <% children.push(column_widget.get_setting_value("#{column_widget.child_widget_setting_prefix(i)}id")) %>
    <% end %>
  </ul>
  <% if column_widget.widgets.count > 0 %>
      <ul>
        <% children.each do |child_id| %>
            <% child = Widget.find(child_id) %>
            <%= render partial: "widget", object: child %>
        <% end %>
      </ul>
  <% end %>
</li>

